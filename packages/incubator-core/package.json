{
  "name": "@incubator/core",
  "version": "0.0.0",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "license": "MIT",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsup src/index.tsx --format esm,cjs --dts --external react",
    "dev": "tsup src/index.tsx --format esm,cjs --watch --dts --external react",
    "lint": "TIMING=1 eslint \"src/**/*.ts*\"",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "devDependencies": {
    "@incubator/tsconfig": "workspace:*",
    "@storybook/theming": "^6.5.16",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.10",
    "eslint": "^8.34.0",
    "eslint-config-incubator": "workspace:*",
    "react": "^18.2.0",
    "tsup": "^6.6.0",
    "typescript": "^4.9.5"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "clsx": "^1.2.1"
  }
}
