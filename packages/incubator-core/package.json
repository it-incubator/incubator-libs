{
  "name": "@incubator/core",
  "version": "0.0.0",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "license": "MIT",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsc && vite build",
    "dev": "concurrently --kill-others \"tsc --watch\" \"vite build --watch\"",
    "lint": "TIMING=1 eslint \"src/**/*.ts*\"",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "devDependencies": {
    "@incubator/tsconfig": "workspace:*",
    "@storybook/theming": "^6.5.16",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.10",
    "@vitejs/plugin-react": "3.0.0",
    "concurrently": "^7.6.0",
    "esbuild-sass-plugin": "^2.4.5",
    "eslint-config-incubator": "workspace:*",
    "prettier-config-incubator": "workspace:*",
    "sass": "^1.58.0",
    "typescript": "^4.9.5",
    "vite": "4.0.4"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@fontsource/roboto": "^4.5.8",
    "@radix-ui/react-collapsible": "^1.0.1",
    "@radix-ui/react-label": "^2.0.0",
    "clsx": "^1.2.1"
  },
  "peerDependencies": {
    "react": ">=17.0.0",
    "react-dom": ">=17.0.0"
  }
}
