    "prettier": "^3.0.3",
    "typescript": "5.2.2",
    "vite": "4.2.1",
    "winston": ">=3.11.0",
    "@nestjs/common": ">=10.2.7",
    "rxjs": ">=7.8.1"
  },
  "peerDependencies": {
    "winston": ">=3.11.0",
    "@nestjs/common": ">=10.2.7",
    "rxjs": ">=7.8.1"
