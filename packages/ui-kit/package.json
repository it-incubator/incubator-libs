{
  "name": "@it-incubator/ui-kit",
  "version": "2.0.11",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "type": "module",
  "types": "./dist/index.d.ts",
  "license": "MIT",
  "sideEffects": false,
  "files": [
    "/dist",
    "/dist/style.css"
  ],
  "scripts": {
    "build": "vite build && tsc && storybook build",
    "lint:eslint": "eslint \"src/**/*.ts*\"",
    "lint:eslint:fix": "pnpm run lint:eslint --fix",
    "lint:stylelint": "node ../stylelint-config/node_modules/stylelint/bin/stylelint.mjs \"src/**/*.scss\"",
    "lint:stylelint:fix": "pnpm run lint:stylelint --fix",
    "lint:types": "tsc",
    "lint": "pnpm run lint:eslint && pnpm run lint:stylelint && pnpm run lint:types",
    "lint:fix": "pnpm run lint:eslint:fix && pnpm run lint:types",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "dev": "storybook dev -p 6006",
    "start": "vite",
    "build-storybook": "storybook build"
  },
  "devDependencies": {
    "@babel/core": "7.22.20",
    "@fontsource/roboto": "5.0.8",
    "@it-incubator/eslint-config": "workspace:*",
    "@it-incubator/prettier-config": "workspace:*",
    "@it-incubator/stylelint-config": "workspace:*",
    "@mdx-js/react": "3.1.1",
    "@storybook/addon-docs": "10.0.7",
    "@storybook/addon-links": "10.0.7",
    "@storybook/react-vite": "10.0.7",
    "@storybook/testing-library": "0.2.2",
    "@types/node": "20.6.4",
    "@types/react": "^19",
    "@types/react-datepicker": "4.15.0",
    "@types/react-dom": "^19",
    "@vitejs/plugin-react": "5.1.1",
    "babel-loader": "9.1.3",
    "concurrently": "8.2.1",
    "date-fns": "2.30.0",
    "esbuild-sass-plugin": "2.15.0",
    "eslint-plugin-storybook": "10.0.7",
    "framer-motion": "^12.23.26",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-toastify": "11.0.5",
    "remark-gfm": "4.0.1",
    "sass": "1.68.0",
    "storybook": "10.0.7",
    "typescript": "5.2.2",
    "vite": "^5"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@headlessui/react": "2.2.9",
    "@radix-ui/react-checkbox": "1.3.3",
    "@radix-ui/react-collapsible": "1.1.12",
    "@radix-ui/react-dialog": "1.1.15",
    "@radix-ui/react-dismissable-layer": "1.0.4",
    "@radix-ui/react-dropdown-menu": "2.1.16",
    "@radix-ui/react-label": "2.1.7",
    "@radix-ui/react-progress": "1.0.3",
    "@radix-ui/react-scroll-area": "1.2.10",
    "@radix-ui/react-select": "1.2.2",
    "@radix-ui/react-slot": "1.2.3",
    "@radix-ui/react-switch": "1.2.6",
    "@radix-ui/react-tabs": "1.1.13",
    "@radix-ui/react-toggle": "1.1.10",
    "@radix-ui/react-tooltip": "1.2.8",
    "clsx": "2.0.0",
    "react-datepicker": "4.19.0",
    "react-dropzone": "14.2.3",
    "react-loading-skeleton": "^3.3.1",
    "react-use-measure": "2.1.1"
  },
  "peerDependencies": {
    "framer-motion": "^10.16.4",
    "react": "^18.2.0 || ^19.0.0",
    "react-dom": "^18.2.0 || ^19.0.0",
    "react-toastify": ">=9.1.1"
  },
  "exports": {
    ".": {
      "react-server": {
        "types": "./dist/server.d.ts",
        "default": "./dist/server.js"
      },
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./dist/style.css": "./dist/style.css",
    "./style.css": "./dist/style.css"
  },
  "eslintignore": [
    "/src/assets/icons/components/*.tsx"
  ]
}
