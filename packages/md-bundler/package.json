{
  "name": "@it-incubator/md-bundler",
  "version": "0.0.9",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "type": "module",
  "types": "./dist/index.d.ts",
  "license": "MIT",
  "sideEffects": false,
  "files": [
    "/dist",
    "/dist/style.css"
  ],
  "scripts": {
    "build": "vite build && tsc",
    "lint:eslint": "eslint \"src/**/*.ts*\"",
    "lint:fix": "eslint \"src/**/*.ts*\" --fix",
    "lint:eslint:fix": "pnpm run lint:eslint --fix",
    "lint:types": "tsc",
    "lint": "TIMING=1 pnpm run lint:eslint && pnpm run lint:types",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "dev": "vite"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@it-incubator/eslint-config": "workspace:*",
    "@it-incubator/prettier-config": "workspace:*",
    "@it-incubator/stylelint-config": "workspace:*",
    "prettier": "^3.0.3",
    "typescript": "5.2.2",
    "vite": "4.2.1"
  },
  "dependencies": {
    "@mdx-js/mdx": "^2.3.0",
    "esbuild": "^0.19.2",
    "hast-util-from-html": "^2.0.1",
    "hast-util-to-mdast": "^10.0.1",
    "mdast-util-from-markdown": "^2.0.0",
    "mdast-util-mdx": "^3.0.0",
    "mdast-util-toc": "^7.0.0",
    "mdx-bundler": "^9.2.1",
    "micromark-extension-mdxjs": "^2.0.0",
    "react": "^18.2.0",
    "rehype-pretty-code": "^0.10.1",
    "rehype-slug": "^6.0.0",
    "remark-gfm": "3.0.1",
    "remark-mdx": "^2.3.0",
    "remark-parse": "^10.0.2",
    "remark-rehype": "^10.1.0",
    "remark-slug": "^7.0.1",
    "remark-toc": "^8.0.1",
    "shiki": "^0.14.4",
    "unified": "^10.1.2"
  }
}